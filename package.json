{
  "name": "@double-great/stylelint-a11y",
  "version": "3.0.0",
  "description": "Plugin for stylelint with a11y rules",
  "main": "dist/index.js",
  "type": "module",
  "exports": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/double-great/stylelint-a11y.git"
  },
  "author": "@double-great",
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "a11y",
    "accessibility"
  ],
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "prepublish": "npm run build",
    "pretest": "npm run lint -- --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "stylelint": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.6",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-stylelint": "^20.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-light-runner": "^0.6.0",
    "jest-preset-stylelint": "^7.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "stylelint": "^16.0.2"
  },
  "bugs": {
    "url": "https://github.com/double-great/stylelint-a11y/issues"
  },
  "homepage": "https://github.com/double-great/stylelint-a11y#readme",
  "dependencies": {
    "postcss": "^8.4.32"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
